{
  "private": true,
  "scripts": {
    "dev-all": "concurrently -m 1 \"npm --section=buildjs run dev\" \"npm --section=buildcss run dev\"  --kill-others-on-fail",
    "prod-all": "concurrently -m 1 \"npm --section=buildjs run prod\" \"npm --section=buildcss run prod\"  --kill-others-on-fail",
    "watch-all": "concurrently \"npm --section=buildjs run watch\" \"npm --section=buildcss run watch\"  --kill-others-on-fail",
    "dev": "npm run development",
    "development": "cross-env NODE_ENV=development node_modules/webpack/bin/webpack.js --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "watch": "npm run development -- --watch",
    "watch-poll": "npm run watch -- --watch-poll",
    "hot": "cross-env NODE_ENV=development node_modules/webpack-dev-server/bin/webpack-dev-server.js --inline --hot --config=node_modules/laravel-mix/setup/webpack.config.js",
    "prod": "npm run production",
    "production": "cross-env NODE_ENV=production node_modules/webpack/bin/webpack.js --no-progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js"
  },
  "devDependencies": {
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "axios": "^0.19",
    "bootstrap": "^4.4.1",
    "clean-webpack-plugin": "^3.0.0",
    "concurrently": "^5.2.0",
    "cross-env": "^7.0",
    "cssnano": "^4.1.10",
    "jquery": "^3.5.1",
    "laravel-mix": "^5.0.1",
    "laravel-mix-merge-manifest": "^0.1.2",
    "lodash": "^4.17.13",
    "popper.js": "^1.12",
    "postcss-fixes": "^2.0.1",
    "resolve-url-loader": "^3.1.0",
    "sass": "^1.26.1",
    "sass-loader": "^8.0.0",
    "vue": "^2.5.17",
    "vue-template-compiler": "^2.6.10"
  },
  "dependencies": {
    "@inertiajs/inertia": "~0.1.9",
    "@inertiajs/inertia-vue": "~0.1.4",
    "sweetalert2": "^9.4.0",
    "vee-validate": "^3.3.0",
    "vue-plugin-load-script": "^1.2.0",
    "vue2-filters": "^0.11.0"
  }
}
